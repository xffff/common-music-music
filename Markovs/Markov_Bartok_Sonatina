define process playtune (reps, pat, rate)
  run repeat reps
    for n = next(pat)
    for r = next(rate)
    send "mp:midi", key: n, dur: r
    wait r
  end

define variable markov_key_voice_one = markov-analyze(key_voice_one, order: 1, mode: 2)
define variable markov_key_voice_two = markov-analyze(key_voice_two, order: 3, mode: 2)
define variable markov_rate_voice_one = markov-analyze(rate_voice_one, order: 5, mode: 2)
define variable markov_rate_voice_two = markov-analyze(rate_voice_two, order: 5, mode: 2)

sprout list (
playtune(2, markov_key_voice_one, markov_rate_voice_one),
playtune(2, markov_key_voice_two, markov_rate_voice_two)
)