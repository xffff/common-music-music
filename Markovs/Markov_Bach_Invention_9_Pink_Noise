define process playtune (reps, pat, rate)
  run repeat reps
    for n = next(pat)
    for r = next(rate)
    if r > 0.
    send "mp:midi", key: n, dur: r
    wait r
  end

define variable markov_key_voice_one = markov-analyze(key_voice_one, order: 4, mode: 2)
define variable markov_key_voice_two = markov-analyze(key_voice_two, order: 4, mode: 2)
define variable markov_rate_voice_one = markov-analyze(rate_voice_one, order: 1, mode: 2)
define variable markov_rate_voice_two = markov-analyze(rate_voice_two, order: 1, mode: 2)

sprout list (
playtune(5000, markov_key_voice_one, markov_rate_voice_one),
playtune(5000, markov_key_voice_two, markov_rate_voice_two)
),"Bach_Invention_9_Pink_2.mid"